<table class="table table-striped table-hover">
  <tr>
    <th><%= t "activerecord.attributes.log_book_event.uuid" %></th>
    <th><%= t "activerecord.attributes.log_book_event.created_at" %></th>
    <th><%= t "activerecord.attributes.log_book_event.historian" %></th>
    <th><%= t "activerecord.attributes.log_book_event.historizable" %></th>
    <th><%= t "activerecord.attributes.log_book_event.tags" %></th>
    <th><%= t "activerecord.attributes.log_book_event.differences" %></th>
  </tr>

  <% log_book_events.each do |log_book_event| %>
    <tr>
      <td><%= log_book_event.id %></td>
      <td><%= log_book_event.created_at.to_fs(:datetimedb) %></td>
      <td><%= log_book_event.historian ? link_to(log_book_event.historian.id, [:admin, log_book_event.historian]) : "-" %></td>
      <td><%= log_book_event.historizable ? link_to(log_book_event.historizable.id, [:admin, log_book_event.historizable]) : "-" %></td>
      <td><%= render_labels log_book_event.tag_list, :tags %></td>
      <td><%= log_book_event.differences ? log_book_event.differences : "-" %></td>
    </tr>
  <% end %>
</table>
